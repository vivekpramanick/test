{
  "name": "gpt4-redis-n8n-chatbot",
  "version": "1.0.0",
  "description": "A comprehensive chatbot system integrating GPT-4, Redis, and n8n",
  "main": "test_chatbot.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
